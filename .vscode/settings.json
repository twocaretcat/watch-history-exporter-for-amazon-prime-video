{
	"search.exclude": {
		"**/.git": true,
		"**/deno.lock": true
	},
	"editor.formatOnSave": true,
	"deno.enable": true,
	"editor.defaultFormatter": "denoland.vscode-deno",
	"[typescript]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	},
	"[javascript]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	},
	"[json]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	}
}
